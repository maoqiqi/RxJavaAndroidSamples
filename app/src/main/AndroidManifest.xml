<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.codearms.maoqiqi.rxjavasamples">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SimpleExampleActivity" />
        <activity android:name=".DisposableExampleActivity" />
        <activity android:name=".SingleObserverExampleActivity" />
        <activity android:name=".CompletableObserverExampleActivity" />
        <activity android:name=".FlowableExampleActivity" />
        <activity android:name=".ReduceExampleActivity" />
        <activity android:name=".ReplayExampleActivity" />
        <activity android:name=".ReplaySubjectExampleActivity" />
        <activity android:name=".PublishSubjectExampleActivity" />
        <activity android:name=".BehaviorSubjectExampleActivity" />
        <activity android:name=".AsyncSubjectExampleActivity" />
        <activity android:name=".ThrottleFirstExampleActivity" />
        <activity android:name=".ThrottleLastExampleActivity" />
        <activity android:name=".TakeWhileExampleActivity" />
        <activity android:name=".TakeUntilExampleActivity" />
        <activity android:name=".CreateActivity" />
        <activity android:name=".TransformActivity" />
        <activity android:name=".FilterActivity" />
        <activity android:name=".CombineActivity" />
        <activity android:name=".ErrorActivity" />
        <activity android:name=".AssistActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>